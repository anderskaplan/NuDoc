        <Issue Name="NoStrongName">
         <Item>'NuDoc.exe'</Item>
        </Issue>
       </Message>
       <Message TypeName="MarkAssembliesWithClsCompliant" Category="Microsoft.Design" CheckId="CA1014" Created="2012-10-21 18:52:58Z">
