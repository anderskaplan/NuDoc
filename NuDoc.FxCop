              <Issue>
               <Item>'fileName'</Item>
               <Item>'DocumentationEngine.WritePublicApiSlashdoc(IAssemblyReflector, Stream, string)'</Item>
              </Issue>
             </Message>
             <Message Id="slashdocStream" TypeName="ReviewUnusedParameters" Category="Microsoft.Usage" CheckId="CA1801" Created="2012-10-21 18:52:58Z" FixCategory="DependsOnFix">
              <Issue>
               <Item>'slashdocStream'</Item>
               <Item>'DocumentationEngine.WritePublicApiSlashdoc(IAssemblyReflector, Stream, string)'</Item>
              </Issue>
             </Message>
